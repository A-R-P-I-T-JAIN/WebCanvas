{
  "rewrites": [
    {
      "source": "/websitegenerator",
      "destination": "/index.html"
    },
    {
      "source": "/sketchtool",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
